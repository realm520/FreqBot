        "use_order_book": false,
        "order_book_top": 1
    },
    "exit_pricing": {
        "price_side": "same", 
        "use_order_book": false,
        "order_book_top": 1
    },
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
