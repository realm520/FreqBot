{
    "trading_mode": "spot",
    "dry_run": true,
    "dry_run_wallet": 10000,
    "cancel_open_orders_on_exit": true,
    "unfilledtimeout": {
        "entry": 10,
        "exit": 30
    },
    "max_open_trades": 10,
    "minimum_trade_amount": 10,
    "tradable_balance_ratio": 0.8,
    "fiat_display_currency": "USD",
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "available_capital": 10000,
    "timeframe": "1m",
